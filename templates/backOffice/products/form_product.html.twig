{% extends 'base_backOffice.twig' %}

{% block body %}
	{# le design est géré dans twig.yalm avec bootstrap #}
	<section>
		<div>
			<h1>FRONTOFFICE</h1>
			<section>
				<div>
					<h1>{{ formTitle }}</h1>
					{{ form_start(formProduct) }}
					{{ form_widget(formProduct) }}
					<button type="submit">{{ formSubmitLabel }}</button>
					{{ form_end(formProduct) }}
				</div>
			</section>
		</div>
	</section>
{% endblock %}
